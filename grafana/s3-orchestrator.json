{
  "uid": "s3-orchestrator",
  "title": "S3 Orchestrator",
  "tags": ["s3-orchestrator", "storage"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "type": "row",
      "id": 1,
      "title": "Overview",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "stat",
      "id": 2,
      "title": "Version",
      "gridPos": { "x": 0, "y": 1, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "s3proxy_build_info",
          "legendFormat": "{{version}}",
          "instant": true
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "name",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "blue", "value": null }]
          }
        }
      }
    },
    {
      "type": "stat",
      "id": 3,
      "title": "Request Rate",
      "gridPos": { "x": 4, "y": 1, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(rate(s3proxy_requests_total[5m]))",
          "legendFormat": "req/s"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          }
        }
      }
    },
    {
      "type": "stat",
      "id": 4,
      "title": "Error Rate %",
      "gridPos": { "x": 8, "y": 1, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(rate(s3proxy_requests_total{status_code=~\"4..|5..\"}[5m])) / sum(rate(s3proxy_requests_total[5m])) * 100",
          "legendFormat": "errors"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "noValue": "0",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "id": 5,
      "title": "Inflight",
      "gridPos": { "x": 12, "y": 1, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(s3proxy_inflight_requests)",
          "legendFormat": "inflight"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "value",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "noValue": "0",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "id": 6,
      "title": "Circuit Breaker",
      "gridPos": { "x": 16, "y": 1, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "s3proxy_circuit_breaker_state",
          "legendFormat": "state",
          "instant": true
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "value",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "noValue": "0",
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": { "text": "Healthy", "color": "green", "index": 0 },
                "1": { "text": "Open", "color": "red", "index": 1 },
                "2": { "text": "Half-Open", "color": "yellow", "index": 2 }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 },
              { "color": "yellow", "value": 2 }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "id": 7,
      "title": "Cleanup Queue",
      "gridPos": { "x": 20, "y": 1, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "s3proxy_cleanup_queue_depth",
          "legendFormat": "pending",
          "instant": true
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "fieldConfig": {
        "defaults": {
          "noValue": "0",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 20 }
            ]
          }
        }
      }
    },
    {
      "type": "row",
      "id": 8,
      "title": "Storage & Quotas",
      "gridPos": { "x": 0, "y": 5, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "gauge",
      "id": 9,
      "title": "Quota Usage",
      "gridPos": { "x": 0, "y": 6, "w": 6, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "max by (backend) (s3proxy_quota_bytes_used) / max by (backend) (s3proxy_quota_bytes_limit) * 100",
          "legendFormat": "{{backend}}"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "orange", "value": 85 },
              { "color": "red", "value": 95 }
            ]
          }
        }
      }
    },
    {
      "type": "table",
      "id": 10,
      "title": "Object Count",
      "gridPos": { "x": 6, "y": 6, "w": 6, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "max by (backend) (s3proxy_objects_count)",
          "format": "table",
          "instant": true
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "backend", "desc": false }]
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "__name__": true },
            "renameByName": { "Value": "Objects" }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Objects" },
            "properties": [
              { "id": "unit", "value": "short" },
              { "id": "custom.displayMode", "value": "color-text" },
              { "id": "color", "value": { "mode": "thresholds" } },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [{ "color": "blue", "value": null }]
                }
              }
            ]
          }
        ]
      }
    },
    {
      "type": "table",
      "id": 11,
      "title": "Available Space",
      "gridPos": { "x": 12, "y": 6, "w": 6, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "max by (backend) (s3proxy_quota_bytes_available)",
          "format": "table",
          "instant": true
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "backend", "desc": false }]
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "__name__": true },
            "renameByName": { "Value": "Available" }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Available" },
            "properties": [
              { "id": "unit", "value": "bytes" },
              { "id": "color", "value": { "mode": "thresholds" } },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    { "color": "red", "value": null },
                    { "color": "yellow", "value": 536870912 },
                    { "color": "green", "value": 1073741824 }
                  ]
                }
              },
              { "id": "custom.displayMode", "value": "color-text" }
            ]
          }
        ]
      }
    },
    {
      "type": "timeseries",
      "id": 12,
      "title": "Storage Over Time",
      "gridPos": { "x": 18, "y": 6, "w": 6, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "max by (backend) (s3proxy_quota_bytes_used)",
          "legendFormat": "{{backend}}"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "scheme",
            "showPoints": "never"
          }
        }
      }
    },
    {
      "type": "row",
      "id": 13,
      "title": "Monthly Usage",
      "gridPos": { "x": 0, "y": 12, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 14,
      "title": "API Requests",
      "gridPos": { "x": 0, "y": 13, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "max by (backend) (s3proxy_usage_api_requests)",
          "legendFormat": "{{backend}}"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull", "max"] }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "scheme",
            "showPoints": "never"
          }
        }
      }
    },
    {
      "type": "timeseries",
      "id": 15,
      "title": "Egress Bytes",
      "gridPos": { "x": 8, "y": 13, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "max by (backend) (s3proxy_usage_egress_bytes)",
          "legendFormat": "{{backend}}"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull", "max"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "scheme",
            "showPoints": "never"
          }
        }
      }
    },
    {
      "type": "timeseries",
      "id": 16,
      "title": "Ingress Bytes",
      "gridPos": { "x": 16, "y": 13, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "max by (backend) (s3proxy_usage_ingress_bytes)",
          "legendFormat": "{{backend}}"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull", "max"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "scheme",
            "showPoints": "never"
          }
        }
      }
    },
    {
      "type": "row",
      "id": 17,
      "title": "Request Performance",
      "gridPos": { "x": 0, "y": 21, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 18,
      "title": "Request Rate by Method",
      "gridPos": { "x": 0, "y": 22, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum by (method, status_code) (rate(s3proxy_requests_total[5m]))",
          "legendFormat": "{{method}} {{status_code}}"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never",
            "stacking": { "mode": "normal" }
          }
        }
      }
    },
    {
      "type": "timeseries",
      "id": 19,
      "title": "Latency Percentiles",
      "gridPos": { "x": 8, "y": 22, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(s3proxy_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(s3proxy_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(s3proxy_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p99"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        }
      }
    },
    {
      "type": "timeseries",
      "id": 20,
      "title": "Throughput",
      "gridPos": { "x": 16, "y": 22, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(rate(s3proxy_request_size_bytes_sum[5m]))",
          "legendFormat": "Upload"
        },
        {
          "expr": "sum(rate(s3proxy_response_size_bytes_sum[5m]))",
          "legendFormat": "Download"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "scheme",
            "showPoints": "never"
          }
        }
      }
    },
    {
      "type": "row",
      "id": 21,
      "title": "Backend Operations",
      "gridPos": { "x": 0, "y": 30, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "id": 22,
      "title": "Backend Request Rate",
      "gridPos": { "x": 0, "y": 31, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum by (operation, backend, status) (rate(s3proxy_backend_requests_total[5m]))",
          "legendFormat": "{{backend}} {{operation}} ({{status}})"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        }
      }
    },
    {
      "type": "timeseries",
      "id": 23,
      "title": "Backend Latency p95",
      "gridPos": { "x": 8, "y": 31, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, operation, backend) (rate(s3proxy_backend_duration_seconds_bucket[5m])))",
          "legendFormat": "{{backend}} {{operation}}"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        }
      }
    },
    {
      "type": "table",
      "id": 24,
      "title": "Active Multipart Uploads",
      "gridPos": { "x": 16, "y": 31, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "max by (backend) (s3proxy_active_multipart_uploads)",
          "format": "table",
          "instant": true
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "backend", "desc": false }]
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "__name__": true },
            "renameByName": { "Value": "Uploads" }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Uploads" },
            "properties": [
              { "id": "unit", "value": "short" },
              { "id": "custom.displayMode", "value": "color-text" },
              { "id": "color", "value": { "mode": "thresholds" } },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    { "color": "green", "value": null },
                    { "color": "yellow", "value": 10 }
                  ]
                }
              }
            ]
          }
        ]
      }
    },
    {
      "type": "row",
      "id": 25,
      "title": "Health & Reliability",
      "gridPos": { "x": 0, "y": 39, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "state-timeline",
      "id": 26,
      "title": "Circuit Breaker Timeline",
      "gridPos": { "x": 0, "y": 40, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "s3proxy_circuit_breaker_state",
          "legendFormat": "state"
        }
      ],
      "options": {
        "showValue": "always",
        "mergeValues": true,
        "alignValue": "center",
        "legend": { "displayMode": "hidden" },
        "tooltip": { "mode": "single" }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": { "text": "Healthy", "color": "green", "index": 0 },
                "1": { "text": "Open", "color": "red", "index": 1 },
                "2": { "text": "Half-Open", "color": "yellow", "index": 2 }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          }
        }
      }
    },
    {
      "type": "timeseries",
      "id": 27,
      "title": "Degraded Mode",
      "gridPos": { "x": 8, "y": 40, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum by (operation) (rate(s3proxy_degraded_reads_total[5m]))",
          "legendFormat": "reads: {{operation}}"
        },
        {
          "expr": "rate(s3proxy_degraded_cache_hits_total[5m])",
          "legendFormat": "cache hits"
        },
        {
          "expr": "sum by (operation) (rate(s3proxy_degraded_write_rejections_total[5m]))",
          "legendFormat": "write rejected: {{operation}}"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "noValue": "0",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never",
            "stacking": { "mode": "normal" }
          }
        }
      }
    },
    {
      "type": "timeseries",
      "id": 28,
      "title": "Cleanup Queue & Rate Limits",
      "gridPos": { "x": 16, "y": 40, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "s3proxy_cleanup_queue_depth",
          "legendFormat": "queue depth"
        },
        {
          "expr": "sum by (reason) (rate(s3proxy_cleanup_queue_enqueued_total[5m]))",
          "legendFormat": "enqueued: {{reason}}"
        },
        {
          "expr": "sum by (status) (rate(s3proxy_cleanup_queue_processed_total[5m]))",
          "legendFormat": "processed: {{status}}"
        },
        {
          "expr": "rate(s3proxy_rate_limit_rejections_total[5m])",
          "legendFormat": "rate limit rejections"
        },
        {
          "expr": "sum by (limit_type) (rate(s3proxy_usage_limit_rejections_total[5m]))",
          "legendFormat": "usage limit: {{limit_type}}"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
